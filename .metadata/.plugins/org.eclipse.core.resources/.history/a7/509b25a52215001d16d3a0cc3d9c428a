package test.com;

public class Main {

	public static void main(String[] args) {
		System.out.println("        ,r'\"7");
		System.out.println("r`-_   ,'  ,/");
		System.out.println("\\. \". L_r'");
		System.out.println("  `~\\/");
		System.out.println("     |");
		System.out.println("     |");
		
		String s = "        ,r'\"7\n" +
				   "r`-_   ,'  ,/\n"  +
				   "\\\\. \\\". L_r'\n" +
				   "  `~\\\\/\n" +
				   "     |" +
				   "     |";
		
		System.out.println(s);
	}

}
