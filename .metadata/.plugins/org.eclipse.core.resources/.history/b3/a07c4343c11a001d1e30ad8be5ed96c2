package homework.com;

class asd {

	public static void main(String[] args) {
		int x = 1;
		while (x < 100) {
			boolean seach = false;
			while (x < 100) {
				if (x%5 == 0 && x%7 == 0) { // 5와 7의 공배수를 찾음
					seach = true;
			    	x++;
			        break; // 
			    }
			}
			
			if(seach) {
				System.out.println(x);
			}
			
		}
		
	}

}
