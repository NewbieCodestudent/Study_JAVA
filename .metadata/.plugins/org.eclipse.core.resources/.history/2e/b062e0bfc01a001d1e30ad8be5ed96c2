package homework.com;

class asd {

	public static void main(String[] args) {
		int i = 1;
		boolean seach = false;
		while (i < 1000) {
			if (i%5 == 0 && i%7 == 0) { // 5와 7의 공배수를 찾음
		    	seach = true;
		        break; // 
		    }
		    i++;
		}
		
		if(seach)
			System.out.println(i);
		else
			System.out.println("찾지 못했습니다.");
	
		
	}

}
