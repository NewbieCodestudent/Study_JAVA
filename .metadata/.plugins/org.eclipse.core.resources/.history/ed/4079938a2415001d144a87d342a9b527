package test.com;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		for (int i = 0; i < b; i++) {
			for (int x = 0; x < a; x++) {
				System.out.print('*');
			}
			System.out.println();
		}
	}

}
